<p id="notice"><%= notice %></p>

<h1>Users</h1>
<br>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'View Profile', user %></td>
        
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
